@using Agency.Models;
@{
    List<PersonDetail> list = (List<PersonDetail>)ViewData["personList"];
    List<Employee> employee = (List<Employee>)ViewData["employeeList"];
    List<Microsoft.AspNetCore.Identity.IdentityRole> roles = (List<Microsoft.AspNetCore.Identity.IdentityRole>)ViewData["role"];
    // var a=0;
    Layout = "~/Views/Shared/_Layout _Admin.cshtml";
}


<h1>Employees</h1>
@if (list != null)
{
    @foreach (var x in list)
    {
        <table class="table thead-dark">
            <tr>
                <td>
                    <input name="id" value="@x.Id" style="display:none" />
                </td>
                <td><label name="first">First name</label></td>
                <td><input type="text" disabled name="firstn" value="@x.FirstName" /></td>
                <td><label name="last">Last name</label></td>
                <td><input type="text" disabled name="lastn" value="@x.LastName" /></td>
                <td><label name="ver">Verified</label></td>
                <td><input type="text" disabled name="vern" value="(@x.Verified)" /></td>

                @*@foreach (var z in city)
                    {
                        <select>
                            <option value="@z.Id">@z.Name</option>
                        </select>
                    }*@
                <td> <a href="/Admin/Update?Id=@x.Id" class="btn btn-dark">Update</a></td>
                <td> <a href="/Admin/Delete?Id=@x.Id" class="btn btn-danger">Delete</a></td>
            </tr>

        </table>

        <br>
    }
}